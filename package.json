  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jodermo/typescipt-repository/issues"
  },
  "homepage": "https://github.com/jodermo/typescipt-repository#readme",
  "devDependencies": {
    "@types/node": "^14.11.2",
    "function-bind": "^1.1.1",
    "gts": "^3.1.1",
    "http": "^0.0.1-security",
    "typescript": "~4.7.0"
  }
}
